{
  "name": "rsl-vsc",
  "displayName": "rsl vsc",
  "description": "rsl syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "rsl",
        "aliases": [
          "Rsl",
          "rsl",
          "Rustle",
          "rustle"
        ],
        "extensions": [
          ".rsl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rsl",
        "scopeName": "source.rsl",
        "path": "./syntaxes/rsl.tmLanguage.json"
      }
    ]
  }
}
